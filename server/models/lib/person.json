{
  "_cms_": {
    "table": {
      "name": "person",
      "columns": ["firstName", "lastName", "email"]
    }
  },

  "firstName": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "notEmpty": true
    },

    "_cms_displayName": "first name"
  },
  "lastName": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "notEmpty": true
    },

    "_cms_displayName": "last name"
  },
  "email": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "isEmail": true
    }
  }
}
