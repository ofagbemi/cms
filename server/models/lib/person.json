{
  "_cms": {
    "table": {
      "name": "person",
      "columns": ["firstName", "lastName", "email"]
    }
  },

  "firstName": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "notEmpty": true
    }
  },
  "lastName": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "notEmpty": true
    }
  },
  "email": {
    "sequelizeType": "STRING",
    "sequelizeValidate": {
      "isEmail": true
    }
  }
}
